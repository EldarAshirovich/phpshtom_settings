<application>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="MAIN_EDITOR" />
    <option name="SOFT_WRAP_FILE_MASKS" value="*.md; *.txt; *.rst; *.adoc, *.php," />
  </component>
  <component name="InlayHintsSettings">
    <option name="lastViewedProviderKeyId" value="parameter.hints.old" />
  </component>
  <component name="PhpCodeFoldingSettings">
    <option name="COLLAPSE_PHP_TAGS" value="true" />
    <option name="COLLAPSE_FUNCTION_BODY" value="true" />
    <option name="COLLAPSE_METHOD_BODY" value="true" />
    <option name="COLLAPSE_CLASSES" value="true" />
  </component>
  <component name="TodoConfiguration">
    <pattern case-sensitive="true" pattern="\btodo\b.*" />
    <pattern case-sensitive="true" pattern="\bfixme\b.*" />
  </component>
</application>